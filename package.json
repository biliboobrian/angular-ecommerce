{
  "name": "angular-ecommerce",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:fr": "ng serve --host 0.0.0.0 --configuration=fr",
    "start:en": "ng serve --host 0.0.0.0 --configuration=en",
    "build": "ng build",
    "build-i18n:fr": "ng build --output-path=dist/fr --aot -prod --bh /fr/ --i18n-file=src/locale/messages.fr.xlf --i18n-format=xlf --locale=fr",
    "build-i18n:en": "ng build --output-path=dist/en --aot -prod --bh /en/ --i18n-file=src/locale/messages.en.xlf --i18n-format=xlf --locale=en",
    "build-i18n": "npm run build-i18n:en && npm run build-i18n:fr",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "int:extract": "ng xi18n --output-path src/locale"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "10.1.1",
    "@angular/common": "10.1.1",
    "@angular/compiler": "10.1.1",
    "@angular/core": "10.1.1",
    "@angular/fire": "6.0.2",
    "@angular/forms": "10.1.1",
    "@angular/localize": "10.1.1",
    "@angular/platform-browser": "10.1.1",
    "@angular/platform-browser-dynamic": "10.1.1",
    "@angular/router": "10.1.1",
    "@ng-bootstrap/ng-bootstrap": "7.0.0",
    "bufferutil": "4.0.1",
    "fibers": "5.0.0",
    "firebase": "7.20.0",
    "html-webpack-plugin": "4.4.1",
    "node-sass": "4.14.1",
    "ol": "6.4.3",
    "rxjs": "6.6.3",
    "tslib": "2.0.1",
    "utf-8-validate": "5.0.2",
    "zone.js": "0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.1001.1",
    "@angular/cli": "10.1.1",
    "@angular/compiler-cli": "10.1.1",
    "@angular/language-service": "10.1.1",
    "@types/jasmine": "3.5.14",
    "@types/jasminewd2": "2.0.8",
    "@types/node": "14.10.1",
    "codelyzer": "6.0.0",
    "jasmine-core": "3.6.0",
    "jasmine-spec-reporter": "5.0.2",
    "karma": "5.2.2",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-jasmine": "4.0.1",
    "karma-jasmine-html-reporter": "1.5.4",
    "npm-normalize-package-bin": "1.0.1",
    "protractor": "7.0.0",
    "ts-node": "9.0.0",
    "tslint": "6.1.3",
    "typescript": "3.9.6",
    "@angular-devkit/architect": "0.1001",
    "firebase-tools": "8.10.0",
    "fuzzy": "0.1.3",
    "inquirer": "6.5.2",
    "inquirer-autocomplete-prompt": "1.1.0"
  }
}
